			"\t\tfin_pour",
			"\tjusqu'à  aucune_permutation",
			"fin",
			"",
			"┌──────────────────────────────────┬────────────────────────┐",
			"│               Objet              │      Nature / Type     │",
			"├──────────────────────────────────┼────────────────────────┤",
			"│ i                                │ entier                 │",
