			"début",
			"\trépéter",
			"\t\taucune_permutation <-- vrai",
			"\t\tpour i de 1 à ${4:N}-1 faire",
			"\t\t\tsi ${2:T}[i-1] ${5|>,<|} ${2:T}[i] alors",
			"\t\t\t\t${6:kes} <-- ${2:T}[i-1]",
			"\t\t\t\t${2:T}[i-1] <-- ${2:T}[i]",
			"\t\t\t\t${2:T}[i] <-- ${6:kes}",
